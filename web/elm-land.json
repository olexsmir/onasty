{
  "app": {
    "elm": {
      "development": {
        "debugger": true
      },
      "production": {
        "debugger": false
      }
    },
    "env": ["FRONTEND_URL"],
    "html": {
      "attributes": {
        "html": {
          "lang": "en",
          "class": "-no-dark-theme"
        },
        "head": {}
      },
      "title": "Onasty",
      "meta": [
        {
          "charset": "UTF-8"
        },
        {
          "http-equiv": "X-UA-Compatible",
          "content": "IE=edge"
        },
        {
          "name": "viewport",
          "content": "width=device-width, initial-scale=1.0"
        }
      ],
      "link": [],
      "script": []
    },
    "router": {
      "useHashRouting": false
    },
    "proxy": {
      "/api": "http://localhost:8000"
    }
  }
}
